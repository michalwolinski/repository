{
    "name": "michalwolinski/repository",
    "description": "Repository Pattern implementation for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michal Wolinski",
            "email": "firma@haxmedia.pl"
        }
    ],
    "minimum-stability": "dev",
    "keywords": [
        "repository", "laravel"
    ],
    "require": {
        "php": "^7.4",
        "laravel/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "MichalWolinski\\Repository\\": "src/",
            "MichalWolinski\\Repository\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichalWolinski\\Repository\\Providers\\RepositoryServiceProvider"
            ]
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^9.0@dev"
    }
}
